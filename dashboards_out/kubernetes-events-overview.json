      "kubernetes-events",
      "kubernetes-events-mixin"
   ],
   "templating": {
      "list": [
         {
            "current": {
               "selected": true,
               "text": "default",
               "value": "default"
            },
            "label": "Data source",
            "name": "datasource",
            "query": "prometheus",
            "type": "datasource"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "hide": 2,
            "label": "Cluster",
            "name": "cluster",
            "query": "label_values(namespace_kind_type:kubernetes_events:count1m{}, cluster)",
            "refresh": 2,
            "sort": 1,
            "type": "query"
         },
         {
            "allValue": ".*",
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "includeAll": true,
            "label": "Event Type",
            "multi": true,
            "name": "type",
            "query": "label_values(namespace_kind_type:kubernetes_events:count1m{cluster=~\"$cluster\"}, type)",
            "refresh": 2,
            "sort": 1,
            "type": "query"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "label": "Kind",
            "name": "kind",
            "query": "label_values(namespace_kind_type:kubernetes_events:count1m{cluster=~\"$cluster\", type=~\"$type\"}, k8s_resource_kind)",
            "refresh": 2,
            "sort": 1,
            "type": "query"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "includeAll": true,
            "label": "Namespace",
            "multi": true,
            "name": "namespace",
            "query": "label_values(namespace_kind_type:kubernetes_events:count1m{cluster=~\"$cluster\", type=~\"$type\", k8s_resource_kind=\"$kind\"}, k8s_namespace_name)",
            "refresh": 2,
            "sort": 1,
            "type": "query"
         }
      ]
   },
